{
  "version": 2,
  "builds": [
    {
      "src": "blogs_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "installCommand": "pip install -r requirements.txt",
        "buildCommand": "python3.9 manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blogs_app/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
